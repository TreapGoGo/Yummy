{
    "manifest_version": 3,
    "name": "Yummy!",
    "version": "0.1.1",
    "description": "精细取舍",
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*",
                "https://chatgpt.com/*"
            ],
            "js": [
                "logger.js",
                "content.js"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_idle"
        }
    ],
    "action": {
        "default_title": "Yummy!"
    },
    "permissions": [
        "storage"
    ]
}